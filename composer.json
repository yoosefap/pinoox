{
    "name": "pinoox/pinoox",
    "description": "pinoox is a simple environment for web management and development. You can do whatever you want on the web and add your desire features to your website in a simple way",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Pinoox",
            "email": "info@pinoox.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
		"php": "^7.3|^8.0",
        "vladimmi/construct-static": "dev-master@dev"
	},
    "autoload": {
        "psr-4": {
            "pinoox\\": "pincore/",
            "pinoox\\app\\": "apps/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pinoox\\tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "9",
        "nunomaduro/collision": "v5.x-dev"
    }
}
