{
    "name": "pinoox/pinoox",
    "description": "pinoox is a simple environment for web management and development. You can do whatever you want on the web and add your desire features to your website in a simple way",
    "keywords": [
        "PHP",
        "PHP Framework"
    ],
    "type": "project",
    "homepage": "http://www.pinoox.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Pinoox",
            "email": "dev@pinoox.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
		"php": "^8.0",
        "vladimmi/construct-static": "dev-master@dev",
        "illuminate/database": "8.x-dev",
        "illuminate/events": "8.x-dev",
        "doctrine/dbal": "3.5.x-dev",
        "symfony/finder": "6.0.x-dev",
        "symfony/http-foundation": "6.0.x-dev",
        "symfony/routing": "6.0.x-dev",
        "symfony/var-dumper": "6.0.x-dev",
        "symfony/http-kernel": "6.0.x-dev",
        "symfony/error-handler": "6.0.x-dev",
        "symfony/dependency-injection": "6.0.x-dev",
        "twig/twig": "3.x-dev",
        "symfony/serializer": "6.0.x-dev",
        "symfony/property-access": "6.0.x-dev",
        "symfony/http-client": "6.0.x-dev",
        "symfony/templating": "6.0.x-dev"
	},
    "autoload": {
        "psr-4": {
            "pinoox\\": "pincore/",
            "pinoox\\app\\": "apps/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pinoox\\test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.27",
        "nunomaduro/collision": "v5.x-dev"
    }
}
