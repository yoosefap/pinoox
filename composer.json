{
    "name": "pinoox/pinoox",
    "description": "pinoox is a simple environment for web management and development. You can do whatever you want on the web and add your desire features to your website in a simple way",
    "keywords": [
        "PHP",
        "PHP Framework",
        "pinoox"
    ],
    "type": "project",
    "homepage": "http://www.pinoox.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Pinoox",
            "email": "dev@pinoox.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "pinoox/pincore": "2.x-dev"
    },
    "autoload": {
        "psr-4": {
            "pinoox\\": "pincore/",
            "pinoox\\app\\": "apps/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pinoox\\test\\": "tests/"
        }
    },
    "bin": ["pinoox"],
    "require-dev": {
        "phpunit/phpunit": "9.5.27",
        "nunomaduro/collision": "^5"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
