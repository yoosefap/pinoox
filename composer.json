{
    "name": "pinoox/pinoox",
    "description": "pinoox is a simple environment for web management and development. You can do whatever you want on the web and add your desire features to your website in a simple way",
    "keywords": [
        "PHP",
        "PHP Framework"
    ],
    "type": "project",
    "homepage": "http://www.pinoox.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Pinoox",
            "email": "dev@pinoox.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
		"php": "^8.0",
        "vladimmi/construct-static": "dev-master@dev",
        "illuminate/database": "8.x-dev",
        "illuminate/events": "8.x-dev",
        "doctrine/dbal": "3.5.x-dev",
        "symfony/finder": "5.4.x-dev"
    },
    "autoload": {
        "files" : [
            "pincore/boot/functions.php"
        ],
        "psr-4": {
            "pinoox\\": "pincore/",
            "pinoox\\app\\": "apps/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pinoox\\tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.27",
        "nunomaduro/collision": "v5.x-dev"
    }
}
